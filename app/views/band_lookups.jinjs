<script type="text/javascript">
$(function() {
    
    /* bandstats objects setup */
    var externalLookup = new Lookup();

    /* event listeners */
    $('#bs-lookup-facebook-id').live('click', function() {
        externalLookup.setProvider('facebook');
        externalLookup.setResource('pageids');
        externalLookup.setService('lookup');
        externalLookup.getMatches();

        return false;
    });
    
});
</script>
